    <xsd:attribute name="HorizontalSpace" type="xsd:string" />
    <xsd:attribute name="TextAlignmentHorizontal">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Right" />
          <xsd:enumeration value="Left" />
          <xsd:enumeration value="Center" />
          <xsd:enumeration value="Justify" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="TextAlignmentVertical">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Top" />
          <xsd:enumeration value="Middle" />
          <xsd:enumeration value="Bottom" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="TextDirection">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Vertical" />
          <xsd:enumeration value="Horizontal" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <!-- TableColumns and TableRows should not apply to TableCell and Table Row -->
