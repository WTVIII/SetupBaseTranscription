		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="none"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="overflow">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="visible"/>
			<xsd:enumeration value="hidden"/>
			<xsd:enumeration value="scroll"/>
			<xsd:enumeration value="auto"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="page-break">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="auto"/>
			<xsd:enumeration value="always"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="position">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="absolute"/>
			<xsd:enumeration value="relative"/>
			<xsd:enumeration value="static"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="relative-size">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="larger"/>
			<xsd:enumeration value="smaller"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="repeat">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="repeat-x"/>
			<xsd:enumeration value="repeat-y"/>
			<xsd:enumeration value="no-repeat"/>
			<xsd:enumeration value="repeat"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="table-layout">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="auto"/>
			<xsd:enumeration value="fixed"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="text-decoration">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="none"/>
			<xsd:enumeration value="overline"/>
			<xsd:enumeration value="underline"/>
			<xsd:enumeration value="line-through"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="text-transform">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="capitalize"/>
			<xsd:enumeration value="uppercase"/>
			<xsd:enumeration value="lowercase"/>
			<xsd:enumeration value="none"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="unicode-bidi">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="normal"/>
			<xsd:enumeration value="embed"/>
			<xsd:enumeration value="bidi-override"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="units">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="pt"/>
			<xsd:enumeration value="in"/>
			<xsd:enumeration value="pc"/>
			<xsd:enumeration value="mm"/>
			<xsd:enumeration value="cm"/>
			<xsd:enumeration value="px"/>
			<xsd:enumeration value="em"/>
			<xsd:enumeration value="ex"/>
			<xsd:enumeration value="%"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="units-values">
		<xsd:restriction base="xsd:decimal">
			<xsd:minInclusive value="-10.00"/>
			<xsd:maxInclusive value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="vertical-align">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="baseline"/>
			<xsd:enumeration value="sub"/>
			<xsd:enumeration value="super"/>
			<xsd:enumeration value="top"/>
			<xsd:enumeration value="middle"/>
			<xsd:enumeration value="bottom"/>
			<xsd:enumeration value="text-top"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="visibility">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="visible"/>
			<xsd:enumeration value="hidden"/>
			<xsd:enumeration value="inherit"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="word-break">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="normal"/>
			<xsd:enumeration value="break-all"/>
			<xsd:enumeration value="keep-all"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- .............................................. -->
	<!-- Style Value Groups -->
	<!-- .............................................. -->
	<xsd:group name="BorderGroup">
		<xsd:choice>
			<xsd:element ref="border-style"/>
			<xsd:element ref="border-width"/>
			<xsd:element ref="color"/>
		</xsd:choice>
	</xsd:group>
	<xsd:group name="Length">
		<xsd:sequence>
			<xsd:element ref="units-values"/>
			<xsd:element ref="units"/>
		</xsd:sequence>
	</xsd:group>
</xsd:schema>
